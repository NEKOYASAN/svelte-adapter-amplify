{
	"compilerOptions": {
		"target": "es2022",
		"module": "es2022",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"noEmit": true,
		"noImplicitAny": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"types": ["./types/ambient.d.ts"],
		"resolveJsonModule": true
	}
}
